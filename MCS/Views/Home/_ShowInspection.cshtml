@using MCS.Models
@model IEnumerable<td_inspection>

<table class="table table-bordered table-condensed table-striped">
    <thead>
        <tr>
            <th>Inspection</th>
            <th>Result</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.tm_inspection.inspection_name</td>
            <td>@if(item.result){<text>OK</text>}else{<text>NG</text>}</td>
            <td>@item.description</td>
        </tr>
        }
        
    </tbody>
</table>